),

//!!! still no tests for multi-channel inputs

]);

knowns = makeTests "known" false;
unknowns = makeTests "unknown" true;

all = [:];
for [ knowns, unknowns ] do h:
    for (keys h) do k: all[k] := h[k] done
done;

all is hash<string, () -> boolean>;

